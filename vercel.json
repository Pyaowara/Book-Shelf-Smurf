{
    "version": 2,
    "builds": [
      {
        "src": "dist/apps/angular-app/**",
        "use": "@vercel/static"
      },
      {
        "src": "backend/src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/backend/(.*)",
        "dest": "/backend/src/main.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  